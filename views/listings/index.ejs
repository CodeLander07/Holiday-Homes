<% layout("/layout/boilerplate") %>
<body>
    <h1>All Listings</h1>
    <form method="GET" action="/listings/new">
      <button>Create New Listing</button>
    </form>
    <ul>
      <% for(let listing of allListings) { %>
        <li>
          <h2><a href="/listings/<%= listing.id %>"><%= listing.title %></a></h2>
        </li>
      <% } %>
    </ul>
</body>
